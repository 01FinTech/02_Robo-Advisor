print ('hello, world')
import numpy as np
import pandas as pd
import datetime
import tushare as ts
df300 = ts.get_hist_data('510300')

##print(df300.head())

print (df300['close'][1:10])

df500 = ts.get_hist_data('510500')

df300a = ts.get_hist_data('150151')
df300b = ts.get_hist_data('150152')

df_result = pd.DataFrame()

df_result['zz500'] = df500['close']
df_result['hs300'] = df300['close']
df_result['hs300a'] = df300a['close']
df_result['hs300b'] = df300b['close']

## calculate 计算
print ( df_result.corr() )



